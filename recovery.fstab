# mount point	fstype		device

/external_sd  vfat      /dev/block/vdd4    /dev/block/vdd
/system       ext4      /dev/block/vda
/data         ext4      /dev/block/vdc     length=-16384
/cache        ext4      /dev/block/vdd3
/boot         emmc      /dev/block/vdd1
/recovery     emmc      /dev/block/vdd2
